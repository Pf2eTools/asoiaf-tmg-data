    "ncus": [
        {
            "id": "30402",
            "name": "Petyr Baelish",
            "subname": "Littlefinger",
            "statistics": {
                "version": "2021-S02",
                "faction": "neutral",
                "abilities": [
                    {
                        "name": "Master of the Game",
                        "effect": [
                            "Petyr Baelish starts the game with 3 Order Tokens.\nEach time Petyr Claims a zone, you may spend 1 Order token to replace its effect with the effect of any unclaimed zone.\nOnce per game, at the start of any Turn, you may select 1 zone. Until the end of the Round, you count as Controlling that zone."
                        ]
                    }
                ]
            }
        },
        {
            "id": "30403",
            "name": "Lord Varys",
            "subname": "The Spider",
            "statistics": {
                "version": "2021-S03",
                "faction": "neutral",
                "abilities": [
                    {
                        "name": "Little Birds",
                        "effect": [
                            "Varys begins the game with 4 Order tokens. \nEach time an enemy NCU Claims a zone, after resolving that zone's effect, you may remove 1 Order token from Varys. If you do, gain the following based on that zone:\n[CROWN]: 1 enemy suffers 1 Panic Test. \n[MONEY]: Restore 2 Wounds to 1 friendly unit.\n[SWORDS]: 1 enemy suffers 3 Hits.\n[LETTER]: Draw 1 Tactics card.\n[HORSE]: 1 friendly unit shifts 3\"."
                        ]
                    }
                ]
            }
        },
        {
            "id": "30404",
            "name": "The High Seneschal",
            "statistics": {
                "version": "2021",
                "faction": "neutral",
                "abilities": [
                    {
                        "name": "History is Written by the Victors",
                        "effect": [
                            "**The High Seneschal cannot activate and cannot be targeted in any way. He merely observes the battle.**\nOnce the victor has been determined, that player may define all actions and events that transpired throughout the game. No opponent may refute or deny any retelling of said events *(regardless of how unlikely or exaggerated)* until such a time where they best the victor in a new game."
                        ]
                    }
                ]
            }
        },
        {
            "id": "30405",
            "name": "Jaqen H'ghar",
            "subname": "Follower of The Red God",
            "statistics": {
                "version": "2021-S03",
                "faction": "neutral",
                "abilities": [
                    {
                        "name": "Choosing a Name",
                        "effect": [
                            "Each time Jaqen Activates, he may replace his Influence Ability with the Influence Ability of any friendly or enemy NCU until the end of the Round."
                        ]
                    },
                    {
                        "name": "A Name Given",
                        "effect": [
                            "**Influence** *(When this unit Claims a zone, attach this card to a Combat Unit until the end of the Round)*:\nWhile Influencing a friendly Infantry unit, its Melee Attacks gain **Precision**."
                        ]
                    }
                ]
            }
        },
        {
            "id": "30406",
            "name": "Tycho Nestoris",
            "subname": "Iron Banker",
            "statistics": {
                "version": "2021-S01",
                "faction": "neutral",
                "abilities": [
                    {
                        "name": "Backing of The Iron Bank",
                        "effect": [
                            "Once per game, at the start of any Turn, you may restore 5 Wounds *(total)* across any number of friendly Combat Units."
                        ]
                    }
                ]
            }
        },
        {
            "id": "30407",
            "name": "Walder Frey",
            "subname": "Lord of the Crossing",
            "statistics": {
                "version": "2021",
                "faction": "neutral",
                "abilities": [
                    {
                        "name": "The Late Walder Frey",
                        "effect": [
                            "You may only Activate Walder if you have no other units that can Activate this Round.\nEach time Walder Claims a zone, you may replace that zone's effect with the effect of any zone. If Walder Claims [CROWN], you may replace that zone's effect with:\n*1 enemy becomes **Weakened**. 1 friendly unit restores 2 Wounds. At the start of the next Round, you become the First Player.*"
                        ]
                    }
                ]
            }
        },
        {
            "id": "30408",
            "name": "Roose Bolton",
            "subname": "Calculating and Cruel",
            "statistics": {
                "version": "2021",
                "faction": "neutral",
                "abilities": [
                    {
                        "name": "Horrific Rumors",
                        "effect": [
                            "Roose begins the game with 3 Order tokens. \nEach time Roose Activates, you may remove 1 Order token from him. \nIf you do, target 1 enemy Combat Unit. They become **Panicked**.\nEach time Roose Claims a zone, you may replace that zone's effect with:\n*1 enemy Combat Unit suffers 1 Panic Test.*"
                        ]
                    }
                ]
            }
        },
        {
            "id": "30409",
            "name": "Shae",
            "subname": "Hidden in Court",
            "statistics": {
                "version": "2021",
                "faction": "neutral",
                "abilities": [
                    {
                        "name": "Finding The Best Patron",
                        "effect": [
                            "Shae begins the game with 3 Order tokens. Each time Shae Claims a zone, after resolving that zone's effect, remove 1 Order token from Shae.\nAt the end of each Round, if Shae has no Order tokens, destroy Shae.\nEach time Shae Claims a zone, before resolving that zone's effect, choose 1:\n  • Restore 1 Wound to 1 friendly Unit.\n  • 1 enemy Unit becomes **Weakened**.\n  • Shae gains 2 Order tokens."
                        ]
                    }
                ]
            }
        }
