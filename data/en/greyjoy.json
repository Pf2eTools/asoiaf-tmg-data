            "faction": "Greyjoy",
            "text": [
                {
                    "trigger": "When a friendly Combat  unit would be destroyed:",
                    "effect": [
                        "That unit is not destroyed,  but instead remains in play  with 1 Wound."
                    ]
                },
                {
                    "trigger": "Start of a friendly Turn:",
                    "effect": [
                        "Target 1 friendly Combat Unit.",
                        "That unit restores 1 Wound,  +1 Wound for each of its  destroyed ranks."
                    ]
                }
            ]
        },
