    "tactics": [
        {
            "id": "yet-another-house-of-harlaw",
            "name": "Yet Another House of Harlaw",
            "statistics": {
                "version": "",
                "faction": "harlaw",
                "text": [
                    {
                        "trigger": "Start of any Round:",
                        "effect": [
                            "If this is the first **Yet Another House of Hawlaw** card to be played, Activate one of your units. That unit performs no Action for its Activation. Once this Tactics Card has been played, it does not go into your discard pile, but is instead put aside.",
                            "If this is the second **Yet Another House of Harlaw** card to be played, immediately deploy 1 Harlaw Reapers unit with 1 Warsworn attached to it fully within Long Range of a friendly table edge, unactivated."
                        ]
                    }
                ]
            }
        },
        {
            "id": "conquest-is-our-harvest",
            "name": "Conquest is Our Harvest",
            "statistics": {
                "version": "",
                "faction": "harlaw",
                "text": [
                    {
                        "trigger": "When a friendly unit is performing a Melee Attack, before rolling Attack Dice:",
                        "effect": [
                            "For each Attachment in this unit, choose 1:",
                            "• This Attack gains **Critical Blow**.",
                            "• The Defender becomes **Panicked**.",
                            "• Target 1 enemy Attachment in the Defender's unit and roll a die. On a 6, destroy that Attachment."
                        ]
                    }
                ]
            }
        },
        {
            "id": "spoils-of-war",
            "name": "Spoils of War",
            "statistics": {
                "version": "",
                "faction": "harlaw",
                "text": [
                    {
                        "trigger": "Start of a fiendly Turn:",
                        "effect": [
                            "Attach this card to 1 friendly Cavalry or Infantry unit until the end of the game.",
                            "While attached, each time an enemy in Short Range is destroyed, place 1 Order token on this card. That unit's Melee Attacks gain the following based on the number of Order tokens *(effects are cumulative)*:",
                            "**1+:** Rolls its Highest Attack Die Value and, before rolling Attack Dice, the Defender becomes **Vulnerable**.",
                            "**2+:** +1 to Hit.**"
                        ]
                    }
                ]
            }
        },
        {
            "id": "we-do-not-sow",
            "name": "We Do Not Sow",
            "statistics": {
                "version": "",
                "faction": "harlaw",
                "text": [
                    {
                        "trigger": "When a friendly NCU Claims a zone:",
                        "effect": [
                            "Replace that zone's effect with:",
                            "*If an opponent Controls [MONEY], this unit restores 1 Wound, +1 Wound for each of its destroyed ranks.*"
                        ]
                    }
                ]
            }
        },
        {
            "id": "the-iron-price",
            "name": "The Iron Price",
            "statistics": {
                "version": "",
                "faction": "harlaw",
                "text": [
                    {
                        "trigger": "Start of a friendly Turn:",
                        "effect": [
                            "Target 1 friendly Combat Unit and choose:",
                            "• That unit may remove 1 Condition token. You may remove 1 Pillage token from that unit. If you do, it restores 2 Wounds, +1 Wound for each of its destroyed ranks.",
                            "• That unit Attacks using its Highest Attack Die Value this Turn. You may remove 1 Pillage token from that unit. If you do, it may re-roll any Attack Dice this Turn."
                        ]
                    }
                ]
            }
        },
        {
            "id": "bless-with-stone-bless-with-steel",
            "name": "Bless with Stone, Bless with Steel",
            "statistics": {
                "version": "",
                "faction": "harlaw",
                "text": [
                    {
                        "trigger": "When a friendly Combat Unit Activates:",
                        "effect": [
                            "Target 1 enemy engaged with that unit. For each of that enemy's destroyed ranks, this unit restores 2 Wounds.",
                            "If you Control [MONEY] or [SWORDS] restore +1 Wound or remove +1 Condition token from that unit for each of those zones you Control."
                        ]
                    }
                ]
            }
        },
        {
            "id": "what-is-dead-may-never-die",
            "name": "What is Dead May Never Die",
            "statistics": {
                "version": "",
                "faction": "harlaw",
                "text": [
                    {
                        "trigger": "When a friendly unit would be destroyed:",
                        "effect": [
                            "That unit is not destroyed, but instead remains in play with 1 Wound."
                        ]
                    },
                    {
                        "trigger": "Start of a friendly Turn:",
                        "effect": [
                            "Target 1 friendly Combat Unit. That unit restores 1 Wound +1 Wound for each of its destroyed ranks."
                        ]
                    }
                ]
            }
        },
